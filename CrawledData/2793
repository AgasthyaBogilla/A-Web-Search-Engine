        $(function () {
            $("#date").datepicker();
        });

        $(document).ready(function () {
            $(window).scroll(function () {
                if ($(window).scrollTop() + $(window).height() == $(document).height()) {
                    $('.go-to-top').hide();
                }
                else {
                    $('.go-to-top').show();
                }
            });

            $('#btn-1').click(function () {
                if ($('#submenu1').hasClass('in')) {
                    $('#menu-sign').removeClass('glyphicon-minus');
                    $('#menu-sign').addClass('glyphicon-plus');
                }
                else {
                    $('#menu-sign').removeClass('glyphicon-plus');
                    $('#menu-sign').addClass('glyphicon-minus');
                }
            })
        });
    </script>

