    <script>
        var headerScript = document.createElement('script');
        headerScript.src = window.engageStorage.getItem('topbarFile');
        document.body.appendChild(headerScript);
    </script>

    <script>
        $('body').on('click', '.modal-info', function (e) {
            e.preventDefault();
            var options = {};
            if ($(this).attr('title')) {
                options.title = $(this).attr('title');
            }
            core.modal.init($($(this).attr('href')), options).open();
        });
    </script>
    <script>
        $(document).ready(function () {
            $('.modal-info').click(function () {
                setTimeout(function () {
                    var content = document.querySelector(
                        '.k-window:not([style*="display: none"]');
                    var tabbable = content.getElementsByTagName('a');
                    var first = tabbable[0];
                    var last = tabbable[tabbable.length - 1];
                    first.focus();
                    content.addEventListener('keydown', function (event) {
                        if (event.shiftKey && event.keyCode === 9 && first == event.target) {
                            event.stopPropagation();
                            event.preventDefault();
                            return false;
                        }
                        if (!event.shiftKey && event.keyCode === 9 && last == event.target) {
                            event.stopPropagation();
                            event.preventDefault();
                            return false;
                        }
                    })
                }, 600);
            });

            $('body').on('click', '.member-modal', function (e) {
                e.preventDefault();
                var card = core.modal.init($('<div />'), {
                    content: $(this).attr('href'),
                    refresh: function () {
                        card.center();
                    }
                }).open();
            });
        });
    </script>
</body>
</html>
